[
    {
        "id": "redeploy-webhook",
        "execute-command": "redeploy.sh",
        "command-working-directory": "./",
        "trigger-rule":
        {
            "and":
            [
                {
                    "match":
                    {
                        "type": "payload-hash-sha1",
                        "secret": "EqQVhJbTPHipKB6N8k87XNr6ifWTyGdLyVRao2XLf2I=",
                        "parameter":
                        {
                            "source": "header",
                            "name": "X-Hub-Signature"
                        }
                    }
                },
                {
                    "match":
                    {
                        "type": "value",
                        "value": "completed",
                        "parameter":
                        {
                            "source": "payload",
                            "name": "workflow_run.conclusion"
                        }
                    }
                }
            ]
        }
    }
]