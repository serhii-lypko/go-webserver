[
  {
      "id": "redeploy-webhook",
      "execute-command": "./redeploy.sh",
      "command-working-directory": "./",
      "trigger-rule":
      {
          "and":
          [
              {
                  "match":
                  {
                      "type": "payload-hash-sha1",
                      "secret": "your-webhook-secret",
                      "parameter":
                      {
                          "source": "header",
                          "name": "X-Hub-Signature"
                      }
                  }
              },
              {
                  "match":
                  {
                      "type": "value",
                      "value": "workflow_run",
                      "parameter":
                      {
                          "source": "payload",
                          "name": "event"
                      }
                  }
              }
          ]
      }
  }
]